    <!-- GDPR Cookie Consent Script -->
    <script>
        (function() {
            // Check if user has already made a choice
            const consent = localStorage.getItem('gdpr-cookie-consent');
            const consentBanner = document.getElementById('gdpr-cookie-consent');
            
            if (!consent) {
                // Show banner after a short delay
                setTimeout(function() {
                    consentBanner.classList.add('show');
                }, 1000);
            }
            
            // Accept button
            document.getElementById('gdpr-accept').addEventListener('click', function() {
                localStorage.setItem('gdpr-cookie-consent', 'accepted');
                localStorage.setItem('gdpr-cookie-consent-date', new Date().toISOString());
                consentBanner.classList.remove('show');
                // Enable analytics and other cookies here
                console.log('Cookies accepted');
            });
            
            // Reject button
            document.getElementById('gdpr-reject').addEventListener('click', function() {
                localStorage.setItem('gdpr-cookie-consent', 'rejected');
                localStorage.setItem('gdpr-cookie-consent-date', new Date().toISOString());
                consentBanner.classList.remove('show');
                // Disable non-essential cookies here
                console.log('Cookies rejected');
            });
            
            // Settings button - open modal
            document.getElementById('gdpr-settings').addEventListener('click', function() {
                document.getElementById('gdpr-settings-modal').classList.add('show');
            });
            
            // Load saved preferences
            function loadCookiePreferences() {
                // Only load if cookie modal exists on this page
                const modal = document.getElementById('gdpr-settings-modal');
                if (!modal) {
                    return; // Exit early if modal doesn't exist
                }
            }
            
            // Save preferences
            function saveCookiePreferences() {
                const preferences = {
                    essential: document.getElementById('cookie-essential').checked,
                    analytics: document.getElementById('cookie-analytics').checked,
                    marketing: document.getElementById('cookie-marketing').checked
                };
                localStorage.setItem('gdpr-cookie-preferences', JSON.stringify(preferences));
                localStorage.setItem('gdpr-cookie-consent', 'custom');
                localStorage.setItem('gdpr-cookie-consent-date', new Date().toISOString());
                
                // Apply preferences
                applyCookiePreferences(preferences);
                
                // Close modal and banner
                document.getElementById('gdpr-settings-modal').classList.remove('show');
                consentBanner.classList.remove('show');
            }
            
            // Apply cookie preferences
            function applyCookiePreferences(prefs) {
                if (prefs.analytics) {
                    // Enable analytics cookies (e.g., Google Analytics)
                    console.log('Analytics cookies enabled');
                } else {
                    // Disable analytics cookies
                    console.log('Analytics cookies disabled');
                }
                
                if (prefs.marketing) {
                    // Enable marketing cookies
                    console.log('Marketing cookies enabled');
                } else {
                    // Disable marketing cookies
                    console.log('Marketing cookies disabled');
                }
            }
            
            // Initialize preferences on page load
            loadCookiePreferences();
            
            // Function to close modal
            function closeCookieModal() {
                const modal = document.getElementById('gdpr-settings-modal');
                if (modal) {
                    modal.classList.remove('show');
                    loadCookiePreferences(); // Reset to saved preferences
                }
            }
            
            // Wait for DOM to be ready before attaching event listeners
            function initCookieModal() {
                const modal = document.getElementById('gdpr-settings-modal');
                const modalContent = modal ? modal.querySelector('.gdpr-modal-content') : null;
                const saveBtn = document.getElementById('gdpr-save-settings');
                const cancelBtn = document.getElementById('gdpr-cancel-settings');
                const closeBtn = document.getElementById('gdpr-modal-close');
                
                // Close button (X)
                if (closeBtn) {
                    closeBtn.onclick = function(e) {
                        e.preventDefault();
                        e.stopPropagation();
                        closeCookieModal();
                        return false;
                    };
                }
                
                // Cancel button
                if (cancelBtn) {
                    cancelBtn.onclick = function(e) {
                        e.preventDefault();
                        e.stopPropagation();
                        closeCookieModal();
                        return false;
                    };
                }
                
                // Save button
                if (saveBtn) {
                    saveBtn.onclick = function(e) {
                        e.preventDefault();
                        e.stopPropagation();
                        saveCookiePreferences();
                        return false;
                    };
                }
                
                // Close modal on background click
                if (modal) {
                    modal.onclick = function(e) {
                        // Only close if clicking directly on the modal backdrop (not on modal-content)
                        if (e.target === modal) {
                            e.preventDefault();
                            closeCookieModal();
                            return false;
                        }
                    };
                    
                    // Prevent modal content clicks from closing the modal
                    if (modalContent) {
                        modalContent.onclick = function(e) {
                            e.stopPropagation();
                        };
                    }
                }
                
                // Close modal on Escape key
                document.addEventListener('keydown', function(e) {
                    if (e.key === 'Escape' || e.keyCode === 27) {
                        if (modal && modal.classList.contains('show')) {
                            e.preventDefault();
                            closeCookieModal();
                        }
                    }
                });
            }
            
            // Initialize modal when DOM is ready
            if (document.readyState === 'loading') {
                document.addEventListener('DOMContentLoaded', initCookieModal);
            } else {
                // Use setTimeout to ensure DOM is fully ready
                setTimeout(initCookieModal, 100);
            }
            
            // Update Accept/Reject to also save preferences
            document.getElementById('gdpr-accept').addEventListener('click', function() {
                const allAccepted = {
                    essential: true,
                    analytics: true,
                    marketing: true
                };
                localStorage.setItem('gdpr-cookie-preferences', JSON.stringify(allAccepted));
                applyCookiePreferences(allAccepted);
            });
            
            document.getElementById('gdpr-reject').addEventListener('click', function() {
                const allRejected = {
                    essential: true,
                    analytics: false,
                    marketing: false
                };
                localStorage.setItem('gdpr-cookie-preferences', JSON.stringify(allRejected));
                applyCookiePreferences(allRejected);
            });
        })();
    </script>

